{
  "name": "integer-divisors",
  "author": "Matthias Schwarz",
  "version": "1.0.0",
  "keywords": [
    "divisor", "factor", "math", "mathematics", "arithmetic", "division", "integer", "number"
  ],
  "description": "Efficient calculation of divisors (also called factors) of small natural numbers",
  "homepage": "https://github.com/vitroxyn/integer-divisors#readme",
  "bugs": "https://github.com/vitroxyn/integer-divisors/issues",

  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "docs": "typedoc --out docs --mode file",
    "coverage": "tsdx test --coverage",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/integer-divisors.esm.js",
  "devDependencies": {
    "husky": "^4.2.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3"
  }
}
